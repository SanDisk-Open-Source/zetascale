ZS_LOG_LEVEL =info
ZS_FLASH_SIZE=16
#ZS_FLASH_FILENAME=/schooner/data/schooner%d
ZS_FLASH_FILENAME=/dev/shm/schooner%d
ZS_LOG_FLUSH_DIR=/dev/shm
ZS_O_DIRECT=0
ZS_REFORMAT=1
ZS_TEST_MODE=1
#The followind reduces amount of junk in the stacktraces
ZS_FLASH_PROTOCOL_THREADS=1
ZS_ASYNC_PUT_THREADS=3
ASYNC_DELETE_CONTAINERS_THREADS=1
ZS_LICENSE_PATH=/root/DEMO

# To enable data checksum, default is disabled
#SDF_OBJECT_DATA_CHECKSUM=1
