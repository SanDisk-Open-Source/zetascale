ZS_LOG_LEVEL=info
ZS_FLASH_SIZE=44
ZS_FLASH_FILENAME=/tmp/schooner0
#ZS_FLASH_FILENAME=/dev/shm/schooner0
#ZS_LOG_FLUSH_DIR=/dev/shm
#ZS_O_DIRECT=1
ZS_REFORMAT=1
ZS_CHECK_MODE=0
#ZS_TEST_MODE=1
ZS_STORM_MODE=1
ZS_TRX_COMMIT_SW=0
ASYNC_DELETE_CONTAINERS=1
#The followind reduces amount of junk in the stacktraces
ZS_FLASH_PROTOCOL_THREADS=1
ZS_ASYNC_PUT_THREADS=3
ASYNC_DELETE_CONTAINERS_THREADS=1

# Async stats
ZS_ASYNC_STATS_ENABLE=1
ZS_ASYNC_STATS_THREADS=4
ASYNC_STATS_SUSPEND_NODE_COUNT=20
# Unit of time is milli secs
ASYNC_STATS_SUSPEND_DURATION=5

#Stats related
ZS_STATS_FILE = /tmp/fdfstats.log
ZS_STATS_DUMP_INTERVAL = 0
ZS_FLUSH_PSTATS_FREQUENCY = 0
# To enable data checksum, default is disabled
#SDF_OBJECT_DATA_CHECKSUM=1

ZS_SCAVENGER_ENABLE=1
ZS_SCAVENGER_THREADS=10
#ZS_SCAVENGER_THROTTLE_VALUE in milli seconds
ZS_SCAVENGER_THROTTLE_VALUE=1

#ZS_SCAVENGE_PER_OBJECTS is used to start scavenger for every ZS_SCAVENGE_PER_OBJECTS deletes
ZS_SCAVENGE_PER_OBJECTS=10000
ZS_FLOG_MODE=ZS_FLOG_FILE_MODE
#ZS_FLOG_MODE=ZS_FLOG_NVRAM_MODE
#ZS_FLOG_NVRAM_FILE=/tmp/nvram_file
#ZS_FLOG_NVRAM_FILE_OFFSET=0x1000

#ZS_SIGNAL_HANDLERS default is 1
#ZS_SIGNAL_HANDLERS=0
#ZS_CACHE_SIZE=800000000

# ZS_CORE_DUMP  default is 1
#ZS_CORE_DUMP 1
ZS_COMPRESSION=0
