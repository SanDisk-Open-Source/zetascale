FDF_LOG_LEVEL =info
FDF_FLASH_SIZE=16
#FDF_FLASH_FILENAME=/schooner/data/schooner%d
FDF_FLASH_FILENAME=/dev/shm/schooner%d
FDF_LOG_FLUSH_DIR=/dev/shm
FDF_O_DIRECT=0
FDF_REFORMAT=1
FDF_TEST_MODE=1
ASYNC_DELETE_CONTAINERS=0
#The followind reduces amount of junk in the stacktraces
FDF_FLASH_PROTOCOL_THREADS=1
FDF_ASYNC_PUT_THREADS=3
ASYNC_DELETE_CONTAINERS_THREADS=1

#Stats related
FDF_STATS_FILE = /tmp/fdfstats.log
FDF_STATS_DUMP_INTERVAL = 5
FDF_LICENSE_PATH=/root/DEMO
#FDF_BLOCK_SIZE = 8192
FDF_FLUSH_PSTATS_FREQUENCY = 100000
# To enable data checksum, default is disabled
#SDF_OBJECT_DATA_CHECKSUM=1

FDF_SCAVENGER_THREADS 10
#FDF_SCAVENGER_THROTTLE_VALUE in milli seconds
FDF_SCAVENGER_THROTTLE_VALUE 1
#FDF_SCAVENGE_PER_OBJECTS is used to start scavenger for every FDF_SCAVENGE_PER_OBJECTS deletes
FDF_SCAVENGE_PER_OBJECTS 10000
